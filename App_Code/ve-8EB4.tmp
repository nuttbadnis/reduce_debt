        If DT_Service.Rows.Count > 0 Then
            ' Define money format.
            Dim MoneyFmt As String = "###,##0.00"
            Dim MoneyRoundup As String = "###,##0"
            ' Define percentage format.
            Dim PercentFmt As String = "#0.00"

            Dim MKTCost As Double
